                @Html.DisplayFor(modelItem => item.NameSucursal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameClient)
            </td>
